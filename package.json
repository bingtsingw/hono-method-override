{
  "name": "hono-method-override",
  "version": "0.2.1",
  "keywords": [
    "hono",
    "bun",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bingtsingw/hono-method-override"
  },
  "license": "MIT",
  "author": {
    "name": "bingtsingw",
    "email": "bingtsingw@gmail.com",
    "url": "https://github.com/bingtsingw"
  },
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "commit": "cz",
    "dev": "tsup --watch",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint:js": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --cache --ext .js,.jsx,.ts,.tsx --fix",
    "mrm": "mrm --preset=@bingtsingw/mrm-preset",
    "prepare": "if [ -d './node_modules/husky' ]; then husky; fi",
    "release": "release-it",
    "test": "bun test",
    "test:dev": "bun test --watch",
    "typecheck": "tsc --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,md,json}": "prettier --check",
    "**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": "@xstools-dev/prettier-config/base",
  "eslintConfig": {
    "extends": "@xstools-dev/eslint-config/base"
  },
  "dependencies": {
    "ufo": "^1.4.0"
  },
  "devDependencies": {
    "@bingtsingw/mrm-preset": "^0.11.1",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@release-it/conventional-changelog": "^8.0.2",
    "@types/bun": "^1.1.12",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "hono": "^4.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "mrm": "^4.1.22",
    "release-it": "^17.10.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "xstools-dev": "^0.0.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "treeshake": true,
    "clean": true,
    "minify": true
  }
}
